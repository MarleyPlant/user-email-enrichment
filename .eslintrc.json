{
    "env": {
        "node": true,
        "browser": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "root": true,
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "warn"
    },
    "plugins": [
        "prettier"
    ]
}